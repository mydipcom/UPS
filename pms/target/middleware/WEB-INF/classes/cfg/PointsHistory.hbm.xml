<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.uswop.pms.dto.TpointsHistory" table="points_history" lazy="false">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native"></generator>
		</id>				
		<property name="userId" type="java.lang.String">
            <column name="userid" length="50" not-null="true"/>
        </property>
        <property name="points" type="java.lang.Integer">
            <column name="points" length="11" not-null="true"/>
        </property>                
        <property name="pointsBalance" type="java.lang.Integer">
        	<column name="pointsbalance" length="11" not-null="true"/>
        </property>
        <property name="message" type="java.lang.String">
        	<column name="message" length="1000" not-null="true"/>
        </property>        
        <property name="status" type="java.lang.Boolean">
            <column name="status"/>
        </property>                           
        <property name="createdTime" type="java.lang.Long">
            <column name="createdtime" length="20" not-null="true"/>              
        </property>        
	</class>
</hibernate-mapping>